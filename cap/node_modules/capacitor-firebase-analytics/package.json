{
  "name": "capacitor-firebase-analytics",
  "version": "0.3.2",
  "description": "Capacitor plugin for iOS and Android for using Firebase Analytics",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Phil Merrell",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest",
    "npm": "^6.7.0"
  },
  "devDependencies": {
    "typescript": "^2.6.2"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorFirebaseAnalytics.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philmerrell/capacitor-firebase-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/philmerrell/capacitor-firebase-analytics.git/issues"
