  "scripts": {
    "ava": "ava --verbose",
    "build": "run-s build:*",
    "build:deptree": "depcruise --exclude \"^node_modules\" --output-type dot src | dot -T svg > site/dist/dependencygraph.svg",
    "build:site": "cd site && npm install && npm run build",
    "ci": "run-s test build",
    "docs": "node ./scripts/docs.js",
    "format": "prettier --write \"{src,scripts,docs,site/src,.circleci,.github}/**/*.{js,md,yml,json,html}\" \"site/*.{js,md}\" \"*.{js,md,yml,json,html}\" \"tests/**/*.js\" \"!CHANGELOG.md\"",
