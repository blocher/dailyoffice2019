    "postpack": "rm -f oclif.manifest.json && npm i",
    "prepack": "oclif-dev manifest && npm prune --prod",
    "prepublishOnly": "git push && git push --tags && gh-release",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "start": "node ./bin/run",
    "test": "run-s test:*",
    "test:ava": "nyc --reporter=lcov ava --verbose && nyc report",
    "test:cli-help": "npm run start -- --help",
    "test:cli-version": "npm run start -- --version",
    "test:format": "prettier --check \"{src,scripts,docs,site/src,.circleci,.github}/**/*.{js,md,yml,json,html}\" \"site/*.{js,md}\" \"*.{js,md,yml,json,html}\" \"tests/**/*.js\" \"!CHANGELOG.md\"",
    "test:lint": "eslint --fix \"{src,scripts}/**/*.js\"",
    "update-semver": "npm --depth 9999 update",
    "version": "npm run docs && auto-changelog -p --template keepachangelog && git add README.md docs CHANGELOG.md",
