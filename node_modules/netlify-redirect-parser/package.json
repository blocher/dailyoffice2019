{
  "name": "netlify-redirect-parser",
  "version": "2.5.0",
  "description": "Parses netlify redirects into a js object representation",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "version": "auto-changelog -p --template keepachangelog && git add CHANGELOG.md",
    "prepublishOnly": "git push && git push --tags && gh-release"
  },
  "keywords": [
    "netlify"
  ],
  "author": "Netlify",
  "license": "MIT",
  "dependencies": {
    "@netlify/config": "^0.11.5",
    "lodash.isplainobject": "^4.0.6"
  },
  "devDependencies": {
    "auto-changelog": "^1.13.0",
    "ava": "^3.6.0",
    "gh-release": "^3.5.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0"
  },
  "files": [
    "common.js",
    "index.js",
    "line-parser.js",
    "netlify-config-parser.js",
    "result.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "prettier --trailing-comma es5 --single-quote --write",
        "git add"
      ],
      "ignore": [
        "**/node_modules/*"
      ]
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/netlify-redirect-parser.git"
  },
  "bugs": {
    "url": "https://github.com/netlify/netlify-redirect-parser/issues"
  },
  "homepage": "https://github.com/netlify/netlify-redirect-parser#readme"
}
